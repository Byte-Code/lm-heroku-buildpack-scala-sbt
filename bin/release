#!/usr/bin/env sh
# Usage
# bin/release BUILD_DIR
# BUILD_DIR=$1

cd $1
unzip -qq target/universal/$SBT_PROJECT_NAME.zip -d target/universal/
chmod +x target/universal/bin/$SBT_PROJECT_NAME
echo "default_process_types:"
echo "  web: target/universal/bin/${SBT_PROJECT_NAME} -Dhttp.port=${PORT} ${JAVA_OPTS}"